


@{
	
	List<Object> menuitems=new List<Object>();
	menuitems.Add(new { text="Daashboard",url="/Transaction"});
	menuitems.Add(new { text="Categories",url="/Category"});
	menuitems.Add(new { text="Transactions",url="/Transaction"});
  
	
